{
    "name": "aiida-gaussian",
    "author": "Kristjan Eimre",
    "author_email": "kristjaneimre@gmail.com",
    "description": "AiiDA plugin for the Gaussian quantum chemistry software",
    "url": "https://github.com/eimrek/aiida-gaussian",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.data": [
            "gaussian = aiida_gaussian.data:DiffParameters"
        ],
        "aiida.calculations": [
            "gaussian = aiida_gaussian.calculations:DiffCalculation"
        ],
        "aiida.parsers": [
            "gaussian = aiida_gaussian.parsers:DiffParser"
        ],
        "aiida.cmdline.data": [
            "gaussian = aiida_gaussian.cli:data_cli"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.0.0,<2.0.0",
        "six",
        "voluptuous"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov"
        ],
        "pre-commit": [
            "astroid==1.6.6; python_version<'3.0'",
            "astroid==2.2.5; python_version>='3.0'",
            "pre-commit==1.20.0",
            "prospector==1.1.7",
            "pylint-django<0.9; python_version<'3.0'",
            "pylint==1.9.4; python_version<'3.0'",
            "pylint==2.3.1; python_version>='3.0'"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
